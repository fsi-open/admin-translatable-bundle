{
    "name": "fsi/admin-translatable-bundle",
    "type": "symfony-bundle",
    "description": "Simple translation mechanisms for fsi-open/admin-bundle",
    "keywords": ["admin", "panel", "symfony", "bundle", "fsi", "translatable", "translations"],
    "license": "MIT",
    "authors": [
        {
            "name": "Artur Czocher",
            "email": "wasper@wasper.pl"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/chives/admin-bundle.git"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "doctrine/persistence": "^1.3|^2.0",
        "fsi/admin-bundle": "^4.0@dev",
        "fsi/datagrid-bundle" : "^2.1.2@dev",
        "fsi/datasource-bundle": "^2.1.3@dev",
        "fsi/doctrine-extensions-bundle": "^2.1.3",
        "symfony/config": "^4.4.12|^5.0",
        "symfony/dependency-injection": "^4.4.12",
        "symfony/form": "^4.4|^5.0",
        "symfony/framework-bundle": "^4.4|^5.0",
        "symfony/http-kernel": "^4.4|^5.0",
        "symfony/options-resolver": "^4.4|^5.0",
        "symfony/routing": "^4.4|^5.0",
        "symfony/translation": "^4.4.12|^5.0",
        "symfony/twig-bridge": "^4.4|^5.0",
        "symfony/validator": "^4.4|^5.0",
        "symfony/twig-bundle": "^4.4|^5.0",
        "symfony/yaml": "^4.4|^5.0"
    },
    "require-dev": {
        "ext-pdo_sqlite": "*",
        "behat/behat": "^3.3",
        "behat/mink": "^1.7",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-extension": "^2.2",
        "behat/mink-selenium2-driver": "^1.3",
        "behat/symfony2-extension": "^2.1",
        "caciobanu/behat-deprecation-extension": "^2.0",
        "doctrine/doctrine-bundle": "^1.6|^2.0",
        "friends-of-phpspec/phpspec-expect": "^3.0|^4.0",
        "friendsofsymfony/ckeditor-bundle": "^1.0|^2.0",
        "fsi/datagrid": "^2.0",
        "fsi/datasource": "^2.0",
        "fsi/resource-repository-bundle": "^2.0",
        "phpspec/phpspec": "^5.0|^6.0|^7.0",
        "phpspec/prophecy": "^1.8",
        "phpstan/phpstan": "^0.12.66",
        "sensiolabs/behat-page-object-extension": "^2.1|^3.0",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/browser-kit": "^4.4|^5.0",
        "symfony/console": "^4.4|^5.0",
        "symfony/css-selector": "^4.4|^5.0",
        "symfony/debug": "^4.4|^5.0",
        "symfony/dom-crawler": "^4.4.12|^5.0",
        "symfony/var-dumper": "^4.4|^5.0",
        "twig/twig": "^2.11"
    },
    "config": {
        "bin-dir": "vendor/bin"
    },
    "autoload": {
        "psr-4": {
            "FSi\\Bundle\\AdminTranslatableBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FSi\\": "features/fixtures/project/src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev",
            "3.1": "3.1-dev",
            "3.0": "3.0-dev",
            "2.1": "2.1-dev",
            "2.0": "2.0-dev"
        }
    }
}
