{
    "name": "fsi/admin-translatable-bundle",
    "type": "symfony-bundle",
    "description": "Simple translation mechanisms for fsi-open/admin-bundle",
    "keywords": ["admin", "panel", "symfony", "bundle", "fsi", "translatable", "translations"],
    "license": "MIT",
    "authors": [
        {
            "name": "Artur Czocher",
            "email": "wasper@wasper.pl"
        }
    ],
    "require": {
        "fsi/admin-bundle": "^3.0",
        "fsi/doctrine-extensions-bundle": "^2.0",
        "php": ">=7.1",
        "symfony/form": "^2.8|^3.0|^4.0",
        "symfony/framework-bundle": "^2.8|^3.0|^4.0",
        "symfony/http-kernel": "^2.8|^3.0|^4.0",
        "symfony/options-resolver": "^2.6|^3.0|^4.0",
        "symfony/routing": "^2.4|^3.0|^4.0",
        "symfony/translation": "^2.6|^3.0|^4.0",
        "symfony/twig-bridge": "^3.0.3|^4.0",
        "symfony/validator": "^2.5|^3.0|^4.0",
        "symfony/twig-bundle": "^2.8|^3.0|^4.0"
    },
    "require-dev": {
        "behat/behat": "^3.3",
        "behat/mink": "^1.7",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-extension": "^2.2",
        "behat/mink-selenium2-driver": "^1.3",
        "behat/symfony2-extension": "^2.1",
        "bossa/phpspec2-expect": "^2.0|^3.0",
        "doctrine/doctrine-bundle": "^1.6",
        "ext-pdo_sqlite": "*",
        "friendsofsymfony/ckeditor-bundle": "^1.0",
        "fsi/resource-repository-bundle": "^2.0",
        "phpspec/phpspec": "^3.4.2|^4.0",
        "phpspec/prophecy": "^1.7.2",
        "sensiolabs/behat-page-object-extension": "^3.0@dev",
        "symfony/browser-kit": "^2.8|^3.0|^4.0",
        "symfony/console": "^2.8|^3.0|^4.0",
        "symfony/css-selector": "^2.8|^3.0|^4.0",
        "symfony/debug": "^2.8|^3.0|^4.0",
        "symfony/dom-crawler": "^2.8|^3.0|^4.0",
        "symfony/var-dumper": "^2.8|^3.0|^4.0",
        "twig/twig": "^1.34|^2.0"
    },
    "config": {
        "bin-dir": "vendor/bin"
    },
    "autoload": {
        "psr-4": {
            "FSi\\Bundle\\AdminTranslatableBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FSi\\FixturesBundle\\": "features/fixtures/project/src/FSi/FixturesBundle"
        },
        "classmap": [ "features/fixtures/project/app/AppKernel.php"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev",
            "3.0": "3.0-dev",
            "2.1": "2.1-dev",
            "2.0": "2.0-dev"
        }
    }
}
